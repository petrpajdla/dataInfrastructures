@article{timpson2014,
  title = {Reconstructing Regional Population Fluctuations in the {{European Neolithic}} Using Radiocarbon Dates: A New Case-Study Using an Improved Method},
  shorttitle = {Reconstructing Regional Population Fluctuations in the {{European Neolithic}} Using Radiocarbon Dates},
  author = {Timpson, Adrian and Colledge, Sue and Crema, Enrico and Edinborough, Kevan and Kerig, Tim and Manning, Katie and Thomas, Mark G. and Shennan, Stephen},
  year = {2014},
  month = dec,
  journal = {Journal of Archaeological Science},
  volume = {52},
  pages = {549--557},
  issn = {0305-4403},
  doi = {10.1016/j.jas.2014.08.011},
  abstract = {In a previous study we presented a new method that used summed probability distributions (SPD) of radiocarbon dates as a proxy for population levels, and Monte-Carlo simulation to test the significance of the observed fluctuations in the context of uncertainty in the calibration curve and archaeological sampling. The method allowed us to identify periods of significant short-term population change, caveated with the fact that around 5\% of these periods were false positives. In this study we present an improvement to the method by applying a criterion to remove these false positives from both the simulated and observed distributions, resulting in a substantial improvement to both its sensitivity and specificity. We also demonstrate that the method is extremely robust in the face of small sample sizes. Finally we apply this improved method to radiocarbon datasets from 12 European regions, covering the period 8000\textendash 4000~BP. As in our previous study, the results reveal a boom-bust pattern for most regions, with population levels rising rapidly after the local arrival of farming, followed by a crash to levels much lower than the peak. The prevalence of this phenomenon, combined with the dissimilarity and lack of synchronicity in the general shapes of the regional SPDs, supports the hypothesis of endogenous causes.},
  langid = {english},
  keywords = {Demography,Europe,Monte Carlo,Neolithic,Radiocarbon,Simulation,SPD,Summed probability distribution},
  file = {/home/knytt/Library/Quantitative Archaeology/Radiocarbon Dates Modelling/Timpson et al_2014_Reconstructing regional population fluctuations in the European Neolithic using.pdf;/home/knytt/Zotero/storage/NPEHJAWE/S0305440314002982.html}
}

