@article{chrisomalis2006,
  title = {Comparing {{Cultures}} and {{Comparing Processes}}: {{Diachronic Methods}} in {{Cross-Cultural Anthropology}}},
  shorttitle = {Comparing {{Cultures}} and {{Comparing Processes}}},
  author = {Chrisomalis, Stephen},
  year = {2006},
