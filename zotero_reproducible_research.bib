@article{bourgeois2017,
  title = {The Impact of Male Burials on the Construction of {{Corded Ware}} Identity: {{Reconstructing}} Networks of Information in the 3rd Millennium {{BC}}},
  shorttitle = {The Impact of Male Burials on the Construction of {{Corded Ware}} Identity},
  author = {Bourgeois, Quentin and Kroon, Erik},
