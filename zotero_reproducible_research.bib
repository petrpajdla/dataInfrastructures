@article{schmidt2022,
  title = {Practices of {{Linked Open Data}} in {{Archaeology}} and {{Their Realisation}} in {{Wikidata}}},
  author = {Schmidt, Sophie C. and Thiery, Florian and Trognitz, Martina},
  year = {2022},
