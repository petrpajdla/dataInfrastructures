@article{nakoinz2018,
  title = {Modelle Und {{Modellierung}} in Der {{Arch\"aologieModels}} and {{Modelling}} in {{Archaeology}}},
  author = {Nakoinz, Oliver},
  year = {2018},
  journal = {{$<$}p{$>$}Historical Social Research / Historische Sozialforschung. Supplement No. 31 (2018): Models and Modelling between Digital and Humanities - A Multidisciplinary Perspective.{$<$}/p{$>$}          {$<$}p{$>$} {$<$}strong{$>$}Starting Point and Frequency: {$<$}/strong{$>$}Year: 1988},
  volume = {Issues per volume: 1},
  pages = {Volumes per year: 1{$<$}/p{$>$}},
  publisher = {{GESIS - Leibniz-Institut f\"ur Sozialwissenschaften}},
  issn = {0936-6784},
  doi = {10.12759/HSR.SUPPL.31.2018.101-112},
  abstract = {Modelle und Modellierung in der Arch\"aologie\guillemotleft. Being a discipline inbetween natural science and humanities, archaeology has conflicting attitudes towards models. On the one hand the term model is currently very fashionable, while on the other hand there is a certain ignorance and even rejection of models in archaeology. This is caused by limited knowledge on models, the polarization of assumed paradigms, and different developments in different subcommunities in archaeology. Models in archaeology range from conceptual social models over typo-chronological models, regression models, network models and 3d models to simulations. One single definition of models seemingly does not work in archaeology, whereas a structured set of different terms based on an overarching definition of models would make sense. Since most models in archaeology are derived from other disciplines, the field would benefit from a trans-disciplinary modelling framework to enable efficient knowledge transfer. In order to establish a fruitful application of diverse modelling frameworks in archaeology, the establishment of disciplinary modelling communities together with a trans-disciplinary modelling community, as well as a proper education in modelling concepts and techniques, is required.},
  collaborator = {{GESIS-Leibniz-Institut F{\"u}r Sozialwissenschaften}},
  copyright = {Creative Commons Attribution 4.0 International},
  langid = {english},
  file = {/home/knytt/Library/Spatial Analysis/Nakoinz_2018_Modelle und Modellierung in der ArchäologieModels and Modelling in Archaeology.pdf}
}

