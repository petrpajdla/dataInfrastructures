@article{westner2020,
  ids = {westner2020a},
  title = {A Novel Combined Approach for Compositional and {{Pb}} Isotope Data of (Leaded) Copper-Based Alloys: Bronze Coinage in {{Magna Graecia}} and {{Rome}} (5th to 2nd Centuries {{BCE}})},
