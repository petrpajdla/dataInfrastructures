@article{kirnbauer2012,
  title = {Post-{{Variscan}} Hydrothermal Vein Mineralization, {{Taunus}}, {{Rhenish Massif}} ({{Germany}}): {{Constraints}} from Stable and Radiogenic Isotope Data},
  shorttitle = {Post-{{Variscan}} Hydrothermal Vein Mineralization, {{Taunus}}, {{Rhenish Massif}} ({{Germany}})},
  author = {Kirnbauer, Thomas and Wagner, Thomas and Taubald, Heiner and Bode, Michael},
  year = {2012},
  month = oct,
  journal = {Ore Geology Reviews},
  volume = {48},
  pages = {239--257},
  issn = {01691368},
  doi = {10.1016/j.oregeorev.2012.03.010},
  abstract = {Post-Variscan hydrothermal base-metal mineralization of the Taunus ore district, SE Rhenish Massif (Germany), has been studied through combination of stable (S, C, O) and radiogenic (Pb) isotope geochemistry. Based on field and textural observations, five hydrothermal mineralization types can be distinguished. These are (1) tetrahedrite\textendash tennantite bearing quartz\textendash ankerite veins, (2) quartz veins with Pb\textendash Zn\textendash Cu ores, (3) giant quartz veins, (4) metasomatic dolomite in Devonian reef complexes, and (5) calcite\textendash (quartz) mineralization in Devonian reefs. The {$\delta$}18OV-SMOW quartz values of base-metal veins are in the range of 18.0\textendash 21.5\textperthousand, whereas those of giant quartz veins have lower values of 15.9\textendash 18.6\textperthousand. This difference reflects the higher fluid fluxes and smaller extent of rock-buffering for the giant quartz veins. Hydrothermal carbonates from the tetrahedrite and Pb\textendash Zn\textendash Cu veins have variable but distinctly negative {$\delta$}13CV-PDB values. They can be explained by contributions from fluids that had picked up low {$\delta$}13CV-PDB carbon via oxidation of organic matter and from fluids that interacted with Devonian reef carbonate having positive {$\delta$}13CV-PDB. Metasomatic dolomite has positive {$\delta$}13CV-PDB values that closely reflect those of the precursor limestone. By contrast, carbonates of calcite\textendash (quartz) mineralization have negative {$\delta$}13CV-PDB values which are negatively correlated with the {$\delta$}18O values. This pattern is explained by fluid mixing processes where contributions from descending cooler fluids with rather low salinity were dominant. The isotope data suggest that tetrahedrite veins, Pb\textendash Zn\textendash Cu veins, and giant quartz veins formed from fluid mixing involving two end-members with contrasting chemical features. This is supported by fluid inclusion data (Adeyemi, 1982) that show repeated alternation between two different types of fluid inclusions, which are hotter intermediate- to high-salinity NaCl\textendash CaCl2 fluids and cooler low-salinity NaCl-dominated fluids. The metal-rich saline fluids were likely generated at the boundary between the pre-Devonian basement and the overlying Devonian\textendash Carboniferous nappe pile. Fault activation resulted in strong fluid focusing and upward migration of large volumes of hot Na\textendash Ca brines, which mixed with cooler and more dilute fluids at shallower crustal levels. Variable contributions from both fluid types, local fluid fluxes, temperature variations, and variations in pH and oxidation state have then controlled the vein mineralogy and metal inventory.},
  langid = {english},
  file = {/home/knytt/Library/Compositional Analysis/Isotopes/Kirnbauer et al_2012_Post-Variscan hydrothermal vein mineralization, Taunus, Rhenish Massif (Germany).pdf}
}

