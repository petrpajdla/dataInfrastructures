@article{depaermentier2020,
  ids = {depaermentier2020a},
  title = {Neolithic Land-Use, Subsistence, and Mobility Patterns in {{Transdanubia}}: {{A}} Multiproxy Isotope and Environmental Analysis from {{Als\'ony\'ek}} \textendash{} {{B\'atasz\'ek}} and {{M\'or\'agy}} \textendash{} {{T\H{u}zk\H{o}domb}}},
  shorttitle = {Neolithic Land-Use, Subsistence, and Mobility Patterns in {{Transdanubia}}},
  author = {Depaermentier, Margaux L. C. and Oszt{\'a}s, Anett and B{\'a}nffy, Eszter and Alt, Kurt W. and Kempf, Michael},
  year = {2020},
  month = oct,
  journal = {Journal of Archaeological Science: Reports},
  volume = {33},
  pages = {102529},
  issn = {2352-409X},
  doi = {10.1016/j.jasrep.2020.102529},
  abstract = {A variety of interdisciplinary research on mobility and migration patterns in Neolithic Hungary has recently contributed to the explanatory models of the Neolithisation across Europe. Most of these models were based on a combination of the spatial distribution of material culture or bioarchaeological and genetic analyses to determine large-scale migration and social or population-dynamic development. This paper aims at contributing to the current discussion by introducing a comprehensive and interdisciplinary multivariate environmental and multiproxy strontium and oxygen isotope analyses in combination with detailed archaeological interpretation of unique Neolithic site-complexes in southern Transdanubia. The integration of historical and modern environmental attributes, bioarchaeological data, and material typology allows for the determination of small- and large-scale mobility patterns and subsistence strategies in southern Hungary.},
