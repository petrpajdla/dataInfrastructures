  author = {Schmid, Clemens},
  year = {2020},
  month = oct,
  journal = {Adaptive Behavior},
  volume = {28},
  number = {5},
  pages = {359--376},
  issn = {1059-7123, 1741-2633},
  doi = {10.1177/1059712319860842},
  abstract = {European Bronze Age archaeology traditionally focusses on two major dimensions to categorise burials \textendash{} although there is an immense variability of attendant phenomena within this spectrum \textendash{} flat graves versus burial mounds and cremation versus inhumation. These traits are an indispensable ingredient for common archaeological narratives of sociocultural interaction and cultural evolution. This article presents a quantitative reconstruction of the general trends in the distribution of Bronze Age burial traditions based on bulk radiocarbon data and employs the resulting time series for the estimation of macro-regional cultural distance. Despite the relatively small amount of input data, the trend reconstruction fits to established archaeological observations for prehistoric Europe. The comparison of cultural and spatial distance leads to the remarkable result of no significant permanent correlation, which indicates that the spread of the relevant aspects of burial traditions cannot be explained with simple diffusion models. Instead, a more complex process of cultural transmission has to be considered.},
  langid = {english},
  file = {/home/knytt/Library/Quantitative Archaeology/Radiocarbon Dates Modelling/Schmid_2020_Evaluating Cultural Transmission in Bronze Age burial rites of Central,.pdf}
}

