  title = {Using {{Gaussian Mixture Model}} Clustering for Multi-Isotope Analysis of Archaeological Fish Bones for Palaeobiodiversity Studies},
  author = {G{\"o}hring, Andrea and Mauder, Markus and Kr{\"o}ger, Peer and Grupe, Gisela},
  year = {2016},
